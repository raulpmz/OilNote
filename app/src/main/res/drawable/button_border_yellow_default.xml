<?xml version="1.0" encoding="utf-8"?>

<layer-list 
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

<item>
    <shape>
		<corners android:radius="3dp"/>
		<padding android:left="1dp" android:right="1dp" android:bottom="1dp" android:top="1dp"/>
	    <solid android:color="#bc7e26"/>
	</shape>
	
</item>


<item>
    <shape>
        <corners android:radius="3dp"/>
		<padding android:bottom="1dp" android:top="1dp"/>
		<gradient android:angle="270" android:startColor="#fbb149" android:centerColor="#f9a42a" android:endColor="#f89609"></gradient>
	</shape>

</item>


</layer-list>