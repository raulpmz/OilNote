<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raul.oilnote">

    <!-- PERMISOS -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="com.megasur.permission.C2D_MESSAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeOilNote">


        <activity
            android:name=".Activitys.Splashscreen"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.LoginActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.MainActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.RegistrationActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.AddWorkerActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Activitys.ListWorkerActivity"
            android:label="@string/title_activity_list_worker"
            android:theme="@style/AppThemeOilNote"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.InfoWorkerActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.EditWorkerActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.ListJornalsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.ListPlotsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.ListWeightsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.ListMissingActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.AddJornalActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.AddPlotActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.EditPlotActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.InfoPlotActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.AddWeightActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.InfoWeightActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.EditWeightActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.ListExpensesActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.AddExpenseActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Activitys.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".Activitys.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.raul.oilnote.Activitys.MainActivity" />
        </activity>
        <activity android:name=".Activitys.OptionsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.ChangePassword"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.TypeJornalActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.InfoTypeJornalActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.InfoAsajaJornal"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activitys.MarginBenefitsActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>